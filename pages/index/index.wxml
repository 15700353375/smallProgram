<!--index.wxml-->
<view class="list-container">
  <view class='list' wx:for="{{alllist}}" wx:for-item="item" data-current="{{item}}"  bindtap='goStatistical'>
    <image src='{{item.url || "http://qsyfw.gnway.cc:12345/static/images/user_1.jpg"}}'></image>
    <view class='list-body'>
      <view class='list-item-title text-ellipsis'>{{item.storeName}}</view>
      <view class='list-item-address text-ellipsis'>
        <!-- <icon type="success" size="12" color="green" /> -->
        <text class="iconfont icon-dizhi-01"></text>
        {{item.address}}
      </view>
      <view class='list-item-phone' data-phone="{{item.phone}}" bindtap='call'>
        <text class='iconfont icon-dianhua'></text>
        {{item.fphone || '028-10086'}}
      </view>
    </view>
  </view>
</view>
