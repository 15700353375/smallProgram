/* packageA/pages/report/report.wxss */
@import '../../../styles/icon.wxss';
@import '../../../styles/icon-block.wxss';
.clearfix::after {
  content: ".";
  clear: both;
  display: block;
  overflow: hidden;
  font-size: 0;
  height: 0;
}
.clearfix {
  zoom: 1;
}



page{
  background: #efeef6;
  width: 100%;
  height: 100%;
  display: flex;
}

.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.main{
  width: 100%;
  flex: 1;
  margin-bottom: 120rpx;
  overflow-y: auto;
}
.bottom-tab{
  height: 120rpx;
  width: 100%;
  background: #eee;
  border-top: 2rpx solid #ddd;
  position: fixed;
  bottom: 0;
  z-index: 99999;
}

